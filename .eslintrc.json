{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",   // Disable unused variables errors
    "@typescript-eslint/no-explicit-any": "off",  // Allow use of 'any' type
    "react/no-unescaped-entities": "off",         // Allow unescaped entities in JSX
    "@typescript-eslint/no-non-null-asserted-optional-chain": "off", // Disable error for non-null assertions in optional chains
    "@typescript-eslint/no-empty-interface": "off"  // Disable empty interface rule (if you're referring to this as no-empty-object-type)
  }
}
